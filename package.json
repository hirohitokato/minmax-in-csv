{
	"name": "jp-katohirohito-extension-vscode-minmax-in-csv",
	"displayName": "MinMax in editor",
	"description": "Highlights min/max value in active editor.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"license": "MIT",
	"keywords": [
		"statistics",
		"summary",
		"decorator",
		"min",
		"max"
	],
	"categories": [
		"Other"
	],
	"bugs": {
		"url": "https://github.com/hirohitokato/minmax-in-csv/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hirohitokato/minmax-in-csv.git"
	},
	"homepage": "https://github.com/hirohitokato/minmax-in-csv",
	"activationEvents": [
		"onLanguage:csv"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [{
			"id": "csv",
			"extensions": [ ".csv" ],
			"aliases": [ "csv", "CSV" ]
		}],
		"commands": [
			{
				"command": "jp-katohirohito-extension-vscode-minmax-in-csv.highlightNumbers",
				"title": "Highlight min/max value."
			}
		],
		"keybindings":[
			{
				"command": "jp-katohirohito-extension-vscode-minmax-in-csv.highlightNumbers",
				"key": "ctrl+shift+e"
			}
		],
		"colors": [
			{
				"id": "minmaxInEditor.minNumberBackground",
				"description": "Background decoration color for min number",
				"defaults": {
					"dark": "#4545aa55",
					"light": "#0055ff55",
					"highContrast": "#0055ff55"
				}
			},
			{
				"id": "minmaxInEditor.maxNumberBackground",
				"description": "Background decoration color for max number",
				"defaults": {
					"dark": "#aa454555",
					"light": "#FF000055",
					"highContrast": "#FF000055"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
